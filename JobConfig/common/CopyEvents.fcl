#
#  fcl to copy events to a new file.  Typically this is run with --nevts =N to create a subset file
#  original author: Dave Brown (LBNL) 2024
#
process_name: CopyEvents
source : { module_type : RootInput }
outputs : {
  CopyOutput : {
    module_type : RootOutput
    fileName    : "Copy.art"
    outputCommands : [ "keep *_*_*_*"]
  }
}
physics : {
  TriggerPath : []
  EndPath : [CopyOutput]
}
physics.trigger_paths : [ TriggerPath ]
physics.end_paths : [ EndPath ]


